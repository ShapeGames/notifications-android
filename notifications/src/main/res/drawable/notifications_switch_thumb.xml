<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:dither="true"
            android:shape="oval"
            android:useLevel="false"
            android:visible="true">
            <solid android:color="#00FFFFFF" />
            <size
                android:width="24dp"
                android:height="24dp" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape
            android:dither="true"
            android:shape="oval"
            android:useLevel="false"
            android:visible="true">
            <solid android:color="#FFFFFF" />
            <size
                android:width="24dp"
                android:height="24dp" />

        </shape>
    </item>
</layer-list>